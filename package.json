{
  "name": "app-name",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.3.6",
    "@aws-amplify/datastore": "^4.3.0",
    "@aws-amplify/ui-components": "^1.9.40",
    "@aws-amplify/ui-react": "^4.6.2",
    "aws-amplify": "^5.2.1",
    "react": "^18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "update": "ncu -u && rm -rf node_modules && rm yarn.lock && yarn",
    "ci": "shadow-cljs release app && webpack && npx karma start --single-run --reporters junit,dots",
    "karma": "karma start",
    "watch": "shadow-cljs watch app browser-test karma-test",
    "build": "shadow-cljs release app",
    "build-verbose": "shadow-cljs release --verbose app",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "debug-build": "shadow-cljs release app --debug",
    "dev-build": "shadow-cljs compile app",
    "repl": "shadow-cljs cljs-repl app",
    "browser-repl": "shadow-cljs browser-repl",
    "clojure-repl": "shadow-cljs clj-repl",
    "test": "shadow-cljs watch test",
    "test-once": "shadow-cljs compile test",
    "nuke": "rm -rf public/js .shadow-cljs node_modules yarn.lock package-lock.json out report.html",
    "shadow-cljs-server": "shadow-cljs server",
    "webpack": "webpack --watch",
    "lint": "clj-kondo --lint ./",
    "pretty": "prettier --write '**/*.*'",
    "format": "yarn pretty && yarn lint "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/react": "^14.0.0",
    "aws-sdk": "^2.1379.0",
    "babel-loader": "^9.1.2",
    "create-react-class": "^15.7.0",
    "cypress": "^12.12.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cljs-test": "^0.1.0",
    "karma-junit-reporter": "^2.0.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "shadow-cljs": "2.23.3",
    "start-server-and-test": "^2.0.0",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1"
  }
}
